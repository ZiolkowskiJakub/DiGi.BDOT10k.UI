<Window x:Class="DiGi.BDOT10k.UI.Application.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiGi.BDOT10k.UI.Application"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="150">
    <Grid>
        <Button x:Name="Load" Content="Load" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="130" Click="Load_Click"/>
        <Button x:Name="Convert" Content="Convert" HorizontalAlignment="Center" Margin="0,35,0,0" VerticalAlignment="Top" Width="130" Click="Convert_Click"/>

    </Grid>
</Window>
